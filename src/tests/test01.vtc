varnishtest "Test samlmeta vmod"

server s1 {
  	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import samlmeta from "${vmod_topbuild}/src/.libs/libvmod_samlmeta.so";

	sub vcl_deliver {
		set resp.http.hello = samlmeta.samlmeta_ttl()
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.hello == ""
}

client c1 -run
